<table class="ui table">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    {{#each accounts.getCurrentUser}}
      <tr>
        <td>
          {{firstname}}
        </td>
        <td>
          {{lastname}}
        </td>
        <td>
          {{email}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
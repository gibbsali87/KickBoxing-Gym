


  <section class="ui segment">
    <h2 class="ui header">
    </h2>
    <table class="ui table">
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        {{#each trainerslists.users}}
          <tr>
            <td>
              {{firstName}}
            </td>
            <td>
              {{lastName}}
            </td>
            <td>
              <a href="accounts/deleteUser/{{id}}" class="ui icon button">
                <i class="icon trash"></i>
              </a>
            </td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </section>
  
    